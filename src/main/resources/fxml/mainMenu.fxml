<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="MainMenu"
      prefHeight="720.0" prefWidth="1280.0"
      stylesheets="@../styles/mainMenu.css" styleClass="Background">
    <Button layoutX="300" layoutY="250" text="new game" styleClass="icons" onMouseClicked="#newGame"/>
    <Button layoutX="300" layoutY="320" text="profile menu" styleClass="icons" onMouseClicked="#profileMenu"/>
    <Button layoutX="300" layoutY="390" text="logout" styleClass="icons" onMouseClicked="#logout"/>
    <Button layoutX="300" layoutY="460" text="exit" styleClass="icons" onMouseClicked="#exit"/>
</Pane>
